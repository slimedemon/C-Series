<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- This key value maps to one of our enum values. -->
		<add key="provider" value="System.Data.SqlClient"/>
	</appSettings>
	
	<!-- Here are the connection strings. -->
	<connectionStrings>
		<add name ="AutoLotSqlProvider" connectionString ="data source=.\SQLEXPRESS;initial catalog=AutoLot;integrated security=True"/>
	</connectionStrings>
	
	<startup>  
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
</configuration>